<h1>Listing devs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Country</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @devs.each do |dev| %>
  <tr>
    <td><%= dev.name %></td>
    <td><%= dev.country %></td>
    <td><%= link_to 'Show', dev %></td>
    <td><%= link_to 'Edit', edit_dev_path(dev) %></td>
    <td><%= link_to 'Destroy', dev, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Dev', new_dev_path %>
